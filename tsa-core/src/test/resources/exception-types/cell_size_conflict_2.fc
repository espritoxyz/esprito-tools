#include "../imports/stdlib.fc";


() recv_internal(int msg_value, cell in_msg_cell, slice in_msg) impure {
    in_msg.end_parse();
    var bits = in_msg~load_bits(100);
    ;; store value so that it won't be optimized out
    var msg = begin_cell()
              .store_slice(bits)
              .end_cell();
    msg~impure_touch();
}

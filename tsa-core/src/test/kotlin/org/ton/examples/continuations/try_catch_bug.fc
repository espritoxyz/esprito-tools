() some_throwing(int op) impure method_id(42) {
    if (op == 1) {
        return ();
    } else {
        throw(1);
    }
}

int use_try_catch(int op) method_id(1337) {
    try {
        some_throwing(op);
        return 1337;
    } catch(_, code) {
        return code;
    }

    ;; Unreachable
    return -1;
}

global int global_value;
(int, int) use_try_catch_complicated_feature(int op) method_id(107) {
    global_value = 10;
    int local_value = 10;

    try {
        ;; Change both global and local values
        global_value = 999;
        local_value = 999;

        ;; Make a `symbolic` exception
        some_throwing(op);
    } catch (_, _) {}

    ;; In case of exception both global and local values must be restored as before executing the `try` block
    return (global_value, local_value);
}

